.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh; /* 使用视口高度确保父容器占满整个屏幕 */
}

.logo {
  width: 120px; /* 根据实际的logo大小调整 */
  height: 120px; /* 根据实际的logo大小调整 */
  margin-bottom: 20px; /* 与文字保持距离 */
}

.text {
  font-size: 16px;
  color: #333; /* 可根据实际需要调整文字颜色 */
}
